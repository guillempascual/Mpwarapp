{% extends "base.twig" %}

{% block title %}
    {{ title }}
{% endblock  %}
{% block content %}
    <div class="col-lg-12 " >
        <h1>All Customers</h1>
        <ul>
            {% for customer in all_customers %}
                <li>
                    <p><h2>{{ customer.name }}</h2></p>
                    <p>email: {{ customer.email }}</p>
                    <p>phone: {{  customer.phone }} </p>
                </li>
            {% endfor %}
        </ul>
    </div>
{% endblock %}